/*---------------------------------------------
	페이지 레이아웃 관련
---------------------------------------------*/
body {margin:0;}
				
				
/*SMS보내기*/	
input[type=radio] {vertical-align:middle; margin-left:5px;}
										
#subContents {width:540px; overflow:hidden; margin-top:20px;}
#subContents .info_m { display:block; color:#222; font-weight:600; font-size:17px; padding:12px 0; border-bottom:1px solid #ddd; margin-bottom:20px;}
#subContents .info_m .stinfo{ float:right; color:#557dc8; font-weight:normal; font-size:11px; display:inline-block; }

#subContents .sms_type {width:100%; overflow:hidden; background-color:#f5f5f5; padding:10px; border-bottom:1px solid #666;}
#subContents .sms_type .t_sms0 { font-size:13px; color:#333; font-weight:bold; float:left; display:inline-block; margin-right:50px; height:24px; padding-top:2px;}
#subContents .sms_type .t_sms { font-size:13px; color:#333; font-weight:bold; float:left; display:inline-block; margin-right:50px; height:26px;}
#subContents .sms_type .time {color:#2e62c1; font-weight:bold; }

input[type=radio] {padding:3px; vertical-align:middle; margint:0 6px;}
.search_select{height:25px; border:1px solid #dcdcdc; color:#555; font-size:13px; vertical-align:middle; margin:0 2px;}

#subContents .sms_in { width:500px; float:left; overflow:hidden;}
#subContents .sms_in .phone {width:189px; height:164px; background-image:url(../images/sms/bg_phone.jpg); float:left; padding:36px 12px 20px 12px;}
#subContents .sms_in .phone .inb { display:block; width:128px; margin:auto; color:#555; font-size:11px; font-weight:bold; margin-top:20px;}
#subContents .sms_in .phone .inb em {color:#06a12a;}

#subContents .sms_out {width:360px; float:right; overflow:hidden;}

#subContents .sms_f {width:255px; margin:auto; overflow:hidden; padding-top:30px; clear:both;}
#subContents .sms_f .sms_ok a {font-size:14px; font-weight:600; text-align:center; color:#fff; background-color:#6284c3; padding:10px 35px; float:left; display:block; margin-right:8px;}
#subContents .sms_f .sms_ok a:hover {background-color:#4781ec;}
#subContents .sms_f .sms_can a {font-size:14px; font-weight:600; text-align:center; color:#fff; background-color:#b1b1b1; padding:10px 30px; float:left; display:block;}
#subContents .sms_f .sms_can a:hover {background-color:#6284c3;}

/*테이블-가로*/
#subContents .man_t {width:100%; border-top:2px solid #555; margin-top:15px;}
#subContents .man_t tr {border:1px solid #c9c9c9;}
#subContents .man_t th {padding:10px 0; color:#555; font-size:13px; background-color:#efefef; text-align:center; border-left:1px solid #c9c9c9;}
#subContents .man_t td {padding:0px 0; color:#555; font-size:13px;font-weight:600; border-left:1px solid #c9c9c9; text-align:center;}
#subContents .man_t .view {padding:10px 5px; color:#555; font-size:12px;font-weight:600; border-left:1px solid #c9c9c9; text-align:center; line-height:16px;}
#subContents .man_t .info {padding:10px 5px; color:#555; font-size:12px; font-weight:normal; border-left:1px solid #c9c9c9; text-align:center; line-height:16px;}
#subContents .man_t .mv a{color:#4781ec;}
#subContents .man_t .mv a:hover{color:#555;}

.search{overflow:hidden; margin:20px 0;}
.search .tit{color:#555; font-size:13px; font-weight:600; float:left; margin-right:3px; display:inline-block; padding-top:4px;}
.search .tit2{color:#555; font-size:13px; font-weight:600; float:left; margin-left:20px; display:inline-block;}


/*버튼*/
.btn_add a {font-size:11px; font-weight:600; text-align:center; color:#fff; background-color:#b1b1b1; padding:6px 8px; float:right; display:block; margin-bottom:3px;}
.btn_add a:hover {background-color:#6284c3;}

.btn_v a {font-size:11px; font-weight:600; text-align:center; color:#fff; background-color:#6284c3;  padding:5px 15px; float:right; display:block;}
.btn_v a:hover {background-color:#4781ec;}

.btn_sms a {font-size:11px; font-weight:600; text-align:center; color:#ffffff; background-color:#b1b1b1; padding:3px 6px 2px 6px; margin-right:4px; display:block; height:17px; float:right;}
.btn_sms a:hover {background-color:#6284c3;}
.btn_sms2 a {font-size:11px; font-weight:600; text-align:center; color:#ffffff; background-color:#b1b1b1; margin-right:4px; padding:3px 6px 2px 6px; display:block; height:17px; float:right;}
.btn_sms2 a:hover {background-color:#6284c3;}
.btn_sms3 a {font-size:11px; font-weight:600; text-align:center; color:#ffffff; background-color:#b1b1b1; margin-left:14px; padding:3px 6px 2px 6px; display:block; height:17px; float:left;}
.btn_sms3 a:hover {background-color:#6284c3;}


input.man{padding:3px; border:1px solid #ccc; font-size:12px; margin-right:3px; vertical-align:middle;}
input.byte{padding:3px; border:0; font-size:12px; margin-right:3px; vertical-align:middle; background-color:#e9e9e9;}

.text_area{border:0; color:#fff; font-size:12px; padding:10px; background-color:transparent; overflow:hidden;}
.tel_area{border:0; color:#222; font-size:12px; padding:10px; background-color:#efefef; overflow:hidden; float:left;}


/*테이블*/
.cu_t {border-top:2px solid #555; margin-bottom:18px;}
.cu_t tr {border-bottom:1px solid #c9c9c9;}
.cu_t th {padding:8px 0; color:#555; font-size:12px; font-weight:600; background-color:#efefef; text-align:center;}
.cu_t td {padding:8px; color:#555; font-size:13px; border:0; text-align:left; font-weight:bold;}
.cu_t td .ti { color:#06a12a; padding:0 7px; font-weight:bold; font-size:13px;}
.cu_t td .exn { display:block; font-weight:bold; font-size:12px; color:#ca8252; line-height:20px; padding-top:5px; clear:both;}

.im_t {border-top:1px solid #c9c9c9; margin-bottom:18px; float:right;}
.im_t tr {border:1px solid #c9c9c9;}
.im_t th {padding:8px 0; color:#555; font-size:12px; font-weight:600; background-color:#efefef; text-align:center; border-left:1px solid #c9c9c9;}
.im_t td {padding:3px; color:#222; font-size:12px; border-left:1px solid #c9c9c9; text-align:center;}




